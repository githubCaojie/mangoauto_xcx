/* components/selectcartype/selectcartype.wxss */
.hdonglist {
  font-size: 32rpx;
  position: relative;
  background: #fff;
  margin-top: 0;
  display: flex;
  min-height: 50rpx;
}
.hdonglist .label{
  width: 160rpx;
  margin-right: 16rpx;
  display: inline-block;
  margin-top: 5rpx;
  color: #515151;
  font-size: 32rpx;
  font-weight: 900;
}
.hdonglist input, .hdonglist view {
  width: 526rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
}
.hdonglist picker{
  width: 526rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
  height: 1.4rem;
  line-height: 1.4rem;
}
.hdonglist picker view.picker{
  width: auto;
}
.hdonglist slider{
  height: 1.4rem;
  line-height: 1.4rem;
  margin: 0;
  padding: 0;
}
.comment-btn{
  width: 600rpx;
  margin: 50rpx auto;
}
.comment-btn button{
  height: 96rpx;
  line-height: 96rpx;
  font-size: 32rpx;
  border-radius: 8rpx;
}
.comment-btn button::after {
  border: 0;
}
.carList-block{
  position: absolute;
  width: 526rpx;
  background: #fff;
  left: 160rpx;
  top: 102rpx;
  z-index: 9;
  border: 1rpx solid #b1b1b1;
  color: #000;
  max-height: 317rpx;
  overflow: auto;
}
.carList-block .carli{
  height: 79rpx;
  line-height: 79rpx;
  text-indent: 15rpx;
  border-bottom: 1rpx solid #b1b1b1;
}
.carList-block .carli:last-child{
  border: none;
}
.brand-list{
  position: fixed;
  width: 100vw;
  height: 100vh;
  bottom: 0;
  left: 0;
  z-index: 99;
  background: #fff;
  display: none
}
.brand-list.actives{
  right: 0;
  opacity: 1;
  display: block
}
.contentList {
	box-sizing: border-box;
  height: 100%;
  position: absolute;
  top: 81rpx;
}
.address_top {
	height: 50rpx;
	line-height: 50rpx;
	background: #faf9fe;
	color: #77777f;
	font-size: 28rpx;
	padding: 0 20rpx;
}

.address_bottom {
	height: 88rpx;
	line-height: 88rpx;
	background: #fff;
	color: #070a13;
	font-size: 28rpx;
	padding: 0 20rpx;
  display: flex;
}
.address_bottom .address_img{
  width: 66rpx;
  height: 66rpx;
  margin: 11rpx 11rpx 11rpx 0;
}
.list-fixed {
	position: absolute;
	width: 100%;
	z-index: 999;
	height: 36px;
	line-height: 36px;
	background: #ebebeb;
	color: #999;
	font-size: 28rpx;
	padding: 0 20rpx;
}

.fixed-title {
	color: #2cc1d1;
}

.hide {
	display: none;
}

.orientation_region {
	padding: 5px 0px;
	width: 45rpx;
	font-size: 20rpx;
	position: absolute;
	top: 50%;
	right: 6rpx;
	transform: translateY(-50%);
  opacity: 0
}
.brand-list.actives .orientation_region{
  opacity: 1
}
.orientation {
	white-space: normal;
	display: inline-block;
	width: 45rpx;
	height: 50rpx;
	font-size: 28rpx;
	font-weight: bold;
	color: rgb(88, 87, 87);
	text-align: center;
}
.orientation_view{
  position: relative;
}
.orientation_city {
	height: 40rpx;
	line-height: 40rpx;
	color: #000;
  text-align: center;
  border-radius: 20rpx;
}
.orientation_mask{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  top: -30rpx;
  left: -150rpx;
  font-size: 50rpx;
  line-height: 100rpx;
  text-align: center;
  color: #000;
}
.orientation_mask::after{
  content: '';
  height: 100%;
  width: 100%;
  border-radius:50% 0 50% 50%;
  background: rgba(0, 0, 0, 0.3);
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  transform: rotate(45deg);
}
.active {
	color: #fbbe01;
}
.carseries-box{
  width: 450rpx;
  height: 100%;
  position: fixed;
  top: 0;
  right: -450rpx;
  border-left: 1rpx solid #d3d3d3;
  background-color: #fff;
  opacity: 0;
  transition: all 0.4s;
  z-index: 999
}
.carseries-box.actives {
  right: 0;
  opacity: 1;
}
.back-carseries{
  height: 8%;
  border-bottom: 1rpx solid #d3d3d3;
  position: absolute;
  top: 0;
  right: 0;
  width: 449rpx;
  background: #fff;
  z-index: 6;
}
.back-carseries .title-txt{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.back-carseries .title-txt text{
  display: block;
  white-space:nowrap;
}
.back-btn{
  width: 80rpx;
  height: 100%;
  background: url(data:image/png;base64,) center center / 50rpx 50rpx no-repeat;
  -moz-transform:rotate(180deg); 
  -webkit-transform:rotate(180deg);
  transform:rotate(180deg);
  filter:progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
}
.carseries-ul,.cartype-ul{
  position: absolute;
  top: 8%;
  right: 0;
  width: 449rpx;
  height: 92%;
  overflow: auto;
}
.carseries-list{
  width: 90%;
  line-height: 80rpx;
  padding: 0 5%;
  background: #fff;
}
.cartype-ul .carseries-list{
  line-height: 40rpx;
  padding: 10rpx 5%;
}
.carseries-list:nth-child(2n){
  background: #dedede;
}
.carseries-all{
  background: #e9f7e8;
  border-top: 1px solid #e0ece2;
  border-bottom: 1px solid #e0ece2;
}
.carpic{
  width: 100%!important;
  border: 1rpx solid #fbbe01;
  border-radius: 20rpx;
  display: flex!important;
  height: 180rpx;
  font-size: 24rpx;
  line-height: 40rpx;
  position: relative;
}
.carpics{
  width: 100%!important;
  border: 1rpx solid #515151!important;
  border-radius: 15rpx;
  line-height: 70rpx;
  text-align: center;
  font-weight: 900;
  font-size: 32rpx!important;
  color: #515151!important;
}
.carpic image{
  width: 260rpx;
  height: 179rpx;
  border-radius: 20rpx;
  position: absolute;
  left: 8rpx;
  top: 50%;
  transform: translate(0,-50%)
}
.carpic .mgprice{
  position: absolute;
  top: -1rpx;
  left: -1rpx;
  font-size: 23rpx;
  background-color: #fbbe01;
  border-top-left-radius: 20rpx;
  width: auto;
  padding: 0rpx 20rpx;
  border-bottom-right-radius: 20rpx;
  display: block;
}
.carname text{
  display: block
}
.hdonglist .carpic view.carname{
  width: 362rpx;
  position: absolute;
  left: 280rpx;
  top: 50%;
  transform: translate(0,-50%)
}
@font-face {
    font-family: "iconfonts";
    src: url('https://at.alicdn.com/t/font_1431657_09vvfvg0ghys.ttf') format('truetype');
}

.iconfonts {
    font-family: "iconfonts" !important;
    font-size: 32rpx;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.icon-guanbi::before {
  content: "\e620"
}
.top{
  height: 80rpx;
  line-height: 80rpx;
  position: fixed;
  background-color: #fff;
  top: 0;
  left: 0;
  border-bottom: 1rpx solid #000;
  text-align: center;
  width: 750rpx;
  z-index: 99;
  display: flex;
}
.top .offList{
  position: absolute;
  top: 0;
  left: 12rpx;
  font-size: 45rpx;
}
.carBrandMain{
  margin-bottom: 81rpx;
}
.top .topTitle{
  width: 750rpx;
  font-size: 30rpx;
}