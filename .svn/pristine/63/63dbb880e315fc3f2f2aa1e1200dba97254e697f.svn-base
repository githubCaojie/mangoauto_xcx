/* pages/outurl/outurl.wxss */
page{
  background-color: #ffffff;
}
page.outurl{
  width: 100%;
  height: 100%;
  position: relative
}
.title{
  font-size: 60rpx;
  width: 750rpx;
  position: absolute;
  left: 0;
  top: 150rpx;
  text-align: center;
  font-weight: bold
}
.label{
  font-size: 28rpx;
  width: 700rpx;
  position: absolute;
  left: 25rpx;
  top: 240rpx;
  text-align: center;
}
.btn{
  width: 600rpx;
  position: absolute;
  bottom: 250rpx;
  left: 50%;
  transform: translate(-50%,0);
  border: none;
  background: #06c360
}