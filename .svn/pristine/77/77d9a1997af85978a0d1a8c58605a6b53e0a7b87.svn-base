/* pages/CarPriceRanking/CarPriceRanking.wxss */
page{
  background: #f5f5f5;
}
.nav{
  height: 80rpx;
  width: 750rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #e5e5e5;
  position: relative;
}
.nav .tab{
  display: flex;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  width: 300rpx;
  margin: 0 auto
}
.nav .tab view{
  width: 120rpx;
  margin: 0 auto;
  text-align: center;
}
.nav .tab view.active{
  border-bottom: 5rpx solid #ff3333;
}
.nav .search{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  right: 2%;
  transform: translate(0%,-50%)
}
.sort{
  display: flex;
  height: 65rpx;
  line-height: 65rpx;
  border-bottom: 1rpx solid #e5e5e5;
  background-color: #fff;
}
.sort label{
  font-weight: 900;
  width: 100rpx;
  text-align: center;
  margin-left: 10rpx;
}
.sort .ul{
  display: flex;
}
.sort .ul text{
  padding: 0 10rpx;
  display: block;
}
.sort .ul text.active{
  color: #f1ab00
}
.content-slide{
  background: #f5f5f5
}
.content-slide .ul .li .car{
  display: flex;
}
.content-slide .ul .li{
  position: relative;
  margin: 20rpx;
  background: #fff;
}
.content-slide .ul .li label{
  position: absolute;
  background: url(data:image/png;base64,) top center / 100% 100% no-repeat;
  color: #fff;
  padding: 0.5% 3%;
  font-size: 20rpx;
  top: -2rpx;
  left: 10rpx;
  position: absolute;
  z-index: 1
}
.content-slide .media{
  display: flex;
  align-items: center;
  overflow: hidden;
  width: 288rpx;
  height: 288rpx;
  position: relative;
}
.content-slide .media image{
  width: 100%;
  height: 75%;
  display: block;
}
.content-slide .media .updatetime{
  position: absolute;
  bottom: 4%;
  left: 0;
  width: 100%;
  text-align: center;
  color: #707070;
  font: 17rpx;
}
.content-slide .carmsg{
  overflow: hidden;
  display: flex;
  flex: 1;
  flex-flow: row wrap;
  align-content: space-between;
  margin: 2%;
}
.content-slide .carmsg .carname{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  color: #333;
  font-weight: 400;
  font-size: 32rpx;
  height: 70rpx;
  line-height: 35rpx;
}
.content-slide .carmsg .carprice{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
  display: flex;
}
.content-slide .carmsg .carprice view{
  line-height: 50rpx;
  margin: 0 auto;
  display: flex;
}
.content-slide .carmsg .carprice image{
  width: 24rpx;
  height: 37.5rpx;
  margin: 6.25rpx 5rpx 6.25rpx 0;
}
.content-slide .carmsg .carprice text{
  font-size: 30rpx;
  font-weight: 600;
  display: block;
  margin-right: 5rpx;
}
.content-slide .carmsg .carsubmit{
  display: flex;
}
.content-slide .carmsg .carsubmit button{
  margin-left: 50rpx;
}
.content-slide .carmsg .carsubmit button:last-child{
  margin-left: 15rpx;
}
.content-slide .carmsg .carsubmit button,.content-slide .carmsg .carsubmit button image{
  width: 164rpx;
  height: 64rpx;
  padding: 0;
  border: none;
}
.content-slide .carmsg .carprice .thisprice,.content-slide .carmsg .carprice .upprice{
  color: #ff3333
}
.content-slide .carmsg .carprice .lastprice{
  font-size: 26rpx;
  color: #999;
  text-decoration: line-through;
  display: flex;
}
.content-slide .carmsg .carprice .downprice{
  color: #2c90dc
}
.loadtip{
  display: block;
  width: 750rpx;
  line-height: 80rpx;
  height: 80rpx;
  text-align: center;
  color: #999;
  border-top: 1rpx solid #ddd;
  font-size: 32rpx;
}
.tanc{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 1
}
.tanc .tancbg{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: #1b2036;
  opacity: 0.8;
}
.getmobile{
  width: 690rpx;
  background-color: #fff;
  position: absolute;
  top: 5%;
  left: 50%;
  transform: translate(-50%,0%);
  border-radius: 8rpx;
}
.getmobile .topimg{
  width: 240rpx;
  height: 62rpx;
  margin: -3rpx auto 0;
}
.getmobile .topname{
  font-size: 32rpx;
  margin: 10rpx 10rpx 0;
  text-align: center;
}
.getmobile .carimg{
  width: 600rpx;
  height: 400rpx;
  margin: 0 auto;
}
.getmobile .carname {
  font-size: 28rpx;
  margin: 0 10rpx;
  padding-bottom: 10rpx;
  text-align: center;
  /* border-bottom: 1rpx solid #cecece; */
}
.getview{
  display: flex;
  margin: 0 50rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.getview label{
  width: 100rpx;
}
.getview slider{
  margin: 0;
  padding: 0;
  width: 490rpx;
  height: 80rpx;
}
.getview input{
  margin: 10rpx 0;
  padding: 0 10rpx;
  width: 390rpx;
  height: 60rpx;
}
.getmobile button{
  padding: 0;
  background-color: #00a3ff;
  color: #fff;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  font-size: 32rpx;
  line-height: 80rpx;
  margin-top: 10rpx;
}
image.loseicon{
  position: absolute;
  bottom: 0;
  right: 60rpx;
  width: 259rpx;
  height: 259rpx;
}