/* pages/Insurance/Insurance.wxss */
page{
  background: #ffffff;
}
.top{
  background-color: #fbbe01;
  padding: 30rpx 50rpx;
}
.top .title{
  font-size: 48rpx;
  font-weight: 900;
  margin-bottom: 10rpx;
}
.top .mobiltList{
  display: flex;
}
.top .mobiltList view{
  margin-right: 30rpx;
  font-size: 28rpx;
  display: flex;
}
.top .mobiltList view .iconfonts{
  font-size: 34rpx;
  margin-left: 5rpx;
}
.tabs{
  display: flex;
  border-bottom: 1rpx solid #d0cfcd;
  background-color: #fff;
}
.tabs.scroll{
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
}
.tabs .tabLi{
  width: 25%;
  text-align: center;
  font-size: 30rpx;
  line-height: 65rpx;
}
.tabs .tabLi.active{
  color: #ffa419;
  font-weight: bold;
  border-bottom: 5rpx solid #ffa419
}
.main{
  width: 700rpx;
  margin: 50rpx auto;
  color: #4f4f4f;
  padding-bottom: 130rpx;
}
.main .title{
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
}
.main .msg{
  text-align: center;
  font-size: 28rpx;
  background-color: #f4f4f4;
  color: #a5a5a5;
  width: 680rpx;
  padding: 20rpx 0;
  border-radius: 10rpx;
  margin: 20rpx auto 30rpx;
}
.main .ul .li{
  display: flex;
}
.main .ul .li .liIcon{
  margin-right: 40rpx;
}
.main .ul .li .liIcon .iconfonts{
  font-size: 60rpx;
  border-radius: 50%;
  width: 74rpx;
  height: 74rpx;
  line-height: 74rpx;
  text-indent: 8rpx;
  border: 3rpx solid #4f4f4f;
}
.main .ul .li .text{
  font-size: 28rpx;
  margin-bottom: 40rpx;
}
.main .ul .li .text .calllist {
  display: flex;
  color: #567eaf
}
.main .ul .li .text .calllist .iconfonts{
  font-size: 34rpx;
  margin-left: 5rpx;
}
.main .ul .li .text .textName{
  font-weight: 600;
  margin-bottom: 10rpx;
}
.main .ul .li .text .msgList{
  line-height: 40rpx;
}
.insuranceList .ul{
  background-color: #f3f3f3;
  position: fixed;
  left: 0;
  bottom: -768rpx;
  z-index: 999;
  width: 100%;
  transition: all 0.4s;
}
.insuranceList .ul.active{
  bottom: 0;
}
.insuranceList .ul .listop{
  background-color: #ffffff;
  display: flex;
}
.insuranceList .ul .listop .title{
  text-align: center;
  font-size: 38rpx;
  line-height: 100rpx;
  border-bottom: 20rpx solid #f3f3f3;
  font-weight: 900;
  width: 100%;
  position: relative;
}
.insuranceList .ul .listop .title .iconfonts{
  font-size: 60rpx;
  position: absolute;
  right: 10rpx;
  top: 50%;
  transform: translate(0,-50%);
  font-weight: normal
}
.insuranceList .ul .uls{
  height: 648rpx;
  background-color: #ffffff;
  overflow: auto;
}
.insuranceList .ul .uls .li{
  border-bottom: 1rpx solid #f3f3f3;
  display: flex;
  line-height: 80rpx;
  padding: 0 30rpx 0 20rpx;
  margin-left: 20rpx;
}
.insuranceList .ul .uls .li image{
  height: 100rpx;
}
.insuranceList .ul .uls .li view{
  width: 50%;
}
.insuranceList .ul .uls .li view.name{
  font-weight: 600
}
.insuranceList .ul .uls .li view.mobile{
  text-align: right;
  color: #567eaf;
}
.menuBox{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 750rpx;
  height: 100rpx;
  border-top: 1rpx solid #f3f3f3;
  background-color: #ffa419;
  color: #ffffff;
}